	margin-bottom:150px
}
.error-search .searchform{
	margin-top:50px
}
.no-search-results{
	max-width:700px;
	text-align:center;
	float:none;
	margin:0 auto;
}
.no-search-results .searchform{
	max-width:500px;
	width:100% !important;
	margin:40px auto !important;
	float:none !important;
}
