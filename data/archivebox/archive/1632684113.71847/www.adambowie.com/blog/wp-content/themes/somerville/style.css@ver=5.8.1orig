	margin-right:5px;
}
select{
	max-width:100%;
	overflow:hidden;
}
/* 10. Search / .searchform****************************************************************************************************************************************************************************************/
.searchform {
	position: relative;
	float: none;
	width:100%;
	margin:0 0 40px 0;
}
.searchSubmit {
	position: absolute;
	height: 50px;
	width: 60px;
	border: none;
	cursor: pointer;
	margin:0;
	overflow:hidden;
	padding:4px !important;
	top: 0;
	right:0;
	font-size:15px !important;
	line-height:1 !important;
}
.searchform input.s {
	padding:15px 5%;
	margin:0 0 0 0;
	height:50px;
	font-style:normal;
	width:100%;
}
.search #content .searchform,.archive #content .searchform {
	margin:0 0 100px 0 ;
	width:70%;
}
.errorentry{
	padding:0 0 100px 0;
}
.errorentry p{
	margin:10px 0 0 0 !important;
}
.search-no-results .blogger .item{
